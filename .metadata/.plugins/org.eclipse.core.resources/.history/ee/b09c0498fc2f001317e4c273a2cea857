package org.cours;


import javax.faces.bean.ManagedBean;
import javax.faces.bean.ManagedProperty;
import javax.faces.bean.SessionScoped;

import org.jboss.logging.Logger;


@ManagedBean(name ="etudiant")
@SessionScoped
public class Etudiant {

	private static final Logger logger = Logger.getLogger(Etudiant.class.getCanonicalName());
	
	@ManagedProperty(value = "Harold")
	private String nom;

	public String getNom() {
		logger.infov("getNom(): {0}", nom);
		return nom;
	}

	public void setNom(String nom) {
		logger.infov("setNom(): {0}", nom);
		this.nom = nom;
	}

	public void debutTraitement() {
		logger.infov("debutTraitement");
	}



}
